{
    "compilerOptions": {
        "outDir": "dist/js",
        "target": "ES5",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "removeComments": true,
        "strict": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "esModuleInterop": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
